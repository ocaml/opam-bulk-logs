ocamlfind ocamlc -c -annot -bin-annot -thread -package ctypes -I src -I test -o src/tgl4.cmi src/tgl4.mli
ocamlfind ocamlopt -c -annot -bin-annot -thread -package ctypes -I src -I test -o src/tgl4.cmx src/tgl4.ml
ocamlfind ocamlopt -a -thread -ccopt -Wl,-no-as-needed -package ctypes.foreign -package ctypes src/tgl4.cmx -o src/tgl4.cmxa
ocamlfind ocamlc -c -annot -bin-annot -thread -package ctypes -I src -I test -o src/tgl4.cmo src/tgl4.ml
ocamlfind ocamlc -a -custom -thread -ccopt -Wl,-no-as-needed -dllib -ltgl3 -package ctypes.foreign -package ctypes src/tgl4.cmo -o src/tgl4.cma
ocamlfind ocamlopt -shared -thread -ccopt -Wl,-no-as-needed -package ctypes.foreign -package ctypes src/tgl4.cmx -o src/tgl4.cmxs
