  mkdir -p /home/opam/.opam/4.01.0/lib/dbm
  ./configure
  make all
  make test
  make install STUBLIBDIR=/home/opam/.opam/4.01.0/lib/stublibs LIBDIR=/home/opam/.opam/4.01.0/lib/dbm
  cp META /home/opam/.opam/4.01.0/lib/dbm
